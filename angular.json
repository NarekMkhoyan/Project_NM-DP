{
  "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
  "version": 1,
  "newProjectRoot": "projects",
  "projects": {
    "ngx-nm-date-picker": {
      "projectType": "library",
      "root": "projects/ngx-nm-date-picker",
      "sourceRoot": "projects/ngx-nm-date-picker/src",
      "prefix": "lib",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:ng-packagr",
          "options": {
            "project": "projects/ngx-nm-date-picker/ng-package.json"
          },
          "configurations": {
            "production": {
              "tsConfig": "projects/ngx-nm-date-picker/tsconfig.lib.prod.json"
            },
            "development": {
              "tsConfig": "projects/ngx-nm-date-picker/tsconfig.lib.json"
            }
          },
          "defaultConfiguration": "production"
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "tsConfig": "projects/ngx-nm-date-picker/tsconfig.spec.json",
            "polyfills": [
              "zone.js",
              "zone.js/testing"
            ]
          }
        }
      }
    },
    "test-dp": {
      "projectType": "application",
      "schematics": {
        "@schematics/angular:component": {
          "style": "scss"
        }
      },
      "root": "projects/test-dp",
      "sourceRoot": "projects/test-dp/src",
      "prefix": "app",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:browser",
          "options": {
            "outputPath": "dist/test-dp/browser",
            "index": "projects/test-dp/src/index.html",
            "main": "projects/test-dp/src/main.ts",
            "polyfills": [
              "zone.js"
            ],
            "tsConfig": "projects/test-dp/tsconfig.app.json",
            "inlineStyleLanguage": "scss",
            "assets": [
              "projects/test-dp/src/favicon.ico",
              "projects/test-dp/src/assets"
            ],
            "styles": [
              "projects/test-dp/src/styles.scss"
            ],
            "scripts": []
          },
          "configurations": {
            "production": {
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "500kb",
                  "maximumError": "1mb"
                },
                {
                  "type": "anyComponentStyle",
                  "maximumWarning": "2kb",
                  "maximumError": "4kb"
                }
              ],
              "outputHashing": "all"
            },
            "development": {
              "buildOptimizer": false,
              "optimization": false,
              "vendorChunk": true,
              "extractLicenses": false,
              "sourceMap": true,
              "namedChunks": true
            }
          },
          "defaultConfiguration": "production"
        },
        "serve": {
          "builder": "@angular-devkit/build-angular:dev-server",
          "configurations": {
            "production": {
              "browserTarget": "test-dp:build:production"
            },
            "development": {
              "browserTarget": "test-dp:build:development"
            }
          },
          "defaultConfiguration": "development"
        },
        "extract-i18n": {
          "builder": "@angular-devkit/build-angular:extract-i18n",
          "options": {
            "browserTarget": "test-dp:build"
          }
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "polyfills": [
              "zone.js",
              "zone.js/testing"
            ],
            "tsConfig": "projects/test-dp/tsconfig.spec.json",
            "inlineStyleLanguage": "scss",
            "assets": [
              "projects/test-dp/src/favicon.ico",
              "projects/test-dp/src/assets"
            ],
            "styles": [
              "projects/test-dp/src/styles.scss"
            ],
            "scripts": []
          }
        },
        "server": {
          "builder": "@angular-devkit/build-angular:server",
          "options": {
            "outputPath": "dist/test-dp/server",
            "main": "projects/test-dp/server.ts",
            "tsConfig": "projects/test-dp/tsconfig.server.json",
            "inlineStyleLanguage": "scss"
          },
          "configurations": {
            "production": {
              "outputHashing": "media"
            },
            "development": {
              "buildOptimizer": false,
              "optimization": false,
              "sourceMap": true,
              "extractLicenses": false,
              "vendorChunk": true
            }
          },
          "defaultConfiguration": "production"
        },
        "serve-ssr": {
          "builder": "@angular-devkit/build-angular:ssr-dev-server",
          "configurations": {
            "development": {
              "browserTarget": "test-dp:build:development",
              "serverTarget": "test-dp:server:development"
            },
            "production": {
              "browserTarget": "test-dp:build:production",
              "serverTarget": "test-dp:server:production"
            }
          },
          "defaultConfiguration": "development"
        },
        "prerender": {
          "builder": "@angular-devkit/build-angular:prerender",
          "options": {
            "routes": [
              "/"
            ]
          },
          "configurations": {
            "production": {
              "browserTarget": "test-dp:build:production",
              "serverTarget": "test-dp:server:production"
            },
            "development": {
              "browserTarget": "test-dp:build:development",
              "serverTarget": "test-dp:server:development"
            }
          },
          "defaultConfiguration": "production"
        }
      }
    }
  }
}
